{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "outDir": "./static/js",
    "rootDir": "./frontend",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "include": ["frontend/**/*"],
  "exclude": ["node_modules", "static"]
}